<core:FragmentDefinition
 xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data" 
    xmlns:viz="sap.viz.ui5.controls" 
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:card="sap.f.cards"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout"
    xmlns:layout="sap.ui.layout"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <!-- <l:BlockLayout background="Dashboard">
	<l:BlockLayoutRow accentCells="Accent1">
		<l:BlockLayoutCell> -->
         <layout:FixFlex id='directMaterialschartFixFlex' minFlexSize="250">
          <layout:fixContent> 
            <VBox class='settingsBox'>
                        <Label text="Filter By" design="Bold" class='settingsLabel'></Label>
                    <Select app:controll = "directMaterials" change = "_handleChangeFilterType">
                        <core:Item key = "01" text = "Monthly"/>
                        <!-- <core:Item key = "02" text = "Weekly"/> -->
                        <core:Item key = "02" text = "Daily"/>
                    </Select>
            </VBox>
          </layout:fixContent>
           <layout:flexContent>
            <viz:Popover id="directMaterialsPopOver"></viz:Popover>
            <viz:VizFrame id="directMaterials" uiConfig="{applicationSet:'fiori'}"
             vizType='vertical_bullet'>
                <viz:dataset>
                    <viz.data:FlattenedDataset data="{detailJSONModel>/directMaterials}">
                        <viz.data:dimensions>
                            <viz.data:DimensionDefinition name="Date"
                                value="{detailJSONModel>Date}" />
                        </viz.data:dimensions>
                        <!-- <viz.data:measures>
                            <viz.data:MeasureDefinition name="amount"
                                value="{detailJSONModel>amount}" />
                        </viz.data:measures> -->
                          <viz.data:measures>
                            <viz.data:MeasureDefinition name="Direct Materials"
                                value="{detailJSONModel>Direct Materials}" />
                            <!-- <viz.data:MeasureDefinition name="Taxes"
                                value="{detailJSONModel>Taxes}" />
                            <viz.data:MeasureDefinition name="Rent"
                                value="{detailJSONModel>Rent}" />
                            <viz.data:MeasureDefinition name="Salariess"
                                value="{detailJSONModel>Salariess}" />
                            <viz.data:MeasureDefinition name="Advertisement"
                                value="{detailJSONModel>Advertisement}" /> -->
                        </viz.data:measures>
                    </viz.data:FlattenedDataset>
                </viz:dataset>

                <viz:feeds>
                       <viz.feeds:FeedItem uid="valueAxis" type="Measure"
                        values="Direct Materials" />
                          <!-- <viz.feeds:FeedItem uid="valueAxis" type="Measure"
                        values="Taxes" />
                          <viz.feeds:FeedItem uid="valueAxis" type="Measure"
                        values="Rent" />
                          <viz.feeds:FeedItem uid="valueAxis" type="Measure"
                        values="Salariess" />
                          <viz.feeds:FeedItem uid="valueAxis" type="Measure"
                        values="Advertisement" /> -->
                    <viz.feeds:FeedItem uid="categoryAxis" type="Dimension"
                        values="Date" />
                </viz:feeds>
            </viz:VizFrame>
            </layout:flexContent>
        </layout:FixFlex>
     <!-- </l:BlockLayoutCell>
			</l:BlockLayoutRow>
		</l:BlockLayout> -->
</core:FragmentDefinition>